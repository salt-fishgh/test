<!--Define the controls for Demo2-->

<page name="Demo 2">
	<control type="text" rect="600 60 800 100">
		<attr name="id" value="0" />
		<attr name="caption" value="****TEST READ WRITE LAYOUT****" />
		<!--show text-->
		<attr name="display" value="Output" />		
	</control>
	
	<control type="button" rect="400 160 800 100">
		<attr name="id" value="1" />
		<attr name="caption" value="Read Channel Analog0_In" />
		<attr name="py_file" value="demo2" />
		<attr name="py_func" value="fill_axis_2dtable" />
		
		<attr name="an_func" value="read" />
		<attr name="channel" value="Analog0_In" />		
	</control>
	
	<control type="button" rect="400 320 800 100">
		<attr name="id" value="2" />
		<attr name="caption" value="Write Channel Analog0_In" />
		<attr name="py_file" value="demo2" />
		<attr name="py_func" value="fill_axis_2dtable" />
		
		<attr name="an_func" value="write" />
		<attr name="channel" value="Analog0_In" />		
	</control>
	
	<control type="text" rect="400 480 800 100">
		<attr name="id" value="3" />
		<attr name="caption" value="Channel Analog0_In" />
		<!--show text-->
		<attr name="display" value="Output" />		
	</control>
	
	<control type="slider" rect="400 500 800 150">
		<!--for slider horizontal-->
		<attr name="style" value="horizontal" />		
		<attr name="id" value="4" />
		<attr name="range" value="0 500 10" /> 
		<attr name="an_func" value="write" />
		<attr name="channel" value="Analog0_In" /> 
	</control>
	
	<control type="editbox" rect="600 660 500 100">
		<attr name="id" value="8" />
		
		<attr name="an_func" value="write" />
		<attr name="channel" value="Analog0_In" />
	</control>
	
	<control type="spinbox" rect="600 820 500 150">
		<attr name="id" value="10" />
		<attr name="range" value="0 100 1" /> 
		<attr name="an_func" value="write" />
		<attr name="channel" value="Analog0_In" /> 
	</control>
</page>

